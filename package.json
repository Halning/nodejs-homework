    "@types/jest": "^29.5.3",
    "axios": "^1.4.0",
    "axios-mock-adapter": "^1.21.5",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^20.4.4",
    "@types/supertest": "^2.0.12",
    "csvtojson": "^2.0.10",
